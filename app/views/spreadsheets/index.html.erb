<h1 class="page-header">Spreadsheets</h1>

<table class="table table-striped">
  <thead>
    <tr>
        <th>
            Spreadsheet ID
        </th>
        <th>
            Batch #
        </th>
          <th>
            File
          </th>
        <th>
            State
        </th>
    </tr>
  </thead>
  <tbody>
    <% @spreadsheets.each do |ss| %>
    <tr>
        <td>
          <%= ss.id %>
        </td>
        <td>
          <%= ss.batch_id %>
        </td>
      <td>
        <%= link_to ss.file_file_name, ss.file.url %>
      </td>
      <td>
        <%= ss.state %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>